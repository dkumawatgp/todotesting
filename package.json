{
  "name": "react-todo-testing-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon server/index.js",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:api": "vitest run --config vitest.config.js",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:coverage:frontend": "vitest run --coverage",
    "test:coverage:backend": "vitest run --config vitest.config.js --coverage",
    "test:coverage:all": "npm run test:coverage:frontend && npm run test:coverage:backend",
    "test:coverage:report": "npm run test:coverage:all && echo 'Coverage reports generated in coverage/ directory'",
    "test:staged": "vitest run --changed",
    "test:staged:api": "vitest run --config vitest.config.js --changed",
    "prepare": "husky install",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "nodemon": "^3.1.11",
    "pend": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.2",
    "husky": "^9.1.7",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.5.2",
    "supertest": "^6.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "vitest related --run"
    ],
    "server/**/*.js": [
      "vitest related --config vitest.config.js --run"
    ]
  }
}
